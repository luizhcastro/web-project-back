generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Participante {
  id               String   @id @default(uuid())
  cpf              String   @unique
  telefone         String
  email            String   @unique
  dataDeNascimento DateTime @map("data_de_nascimento")

  @@map("participante")
}
